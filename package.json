{
  "name": "mano-toolkit",
  "version": "0.1.0",
  "description": "mano-toolkit component library",
  "private": true,
  "main": "dist/index.js",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.6",
    "react-spring": "^6.1.8"
  },
  "peerDependencies": {
    "react": "16.4.2",
    "react-dom": "16.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.0.1",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@svgr/webpack": "2.4.1",
    "autoprefixer": "9.1.5",
    "babel-cli": "6.26.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-named-asset-import": "0.2.2",
    "babel-preset-flow": "6.23.0",
    "babel-preset-minify": "0.5.0",
    "babel-preset-react-app": "5.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-react-app": "3.0.3",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "flow-bin": "0.82.0",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "husky": "1.1.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-enzyme": "6.1.2",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "node-sass": "4.9.3",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.6.3",
    "react-app-polyfill": "0.1.3",
    "react-dev-utils": "^6.1.1",
    "react-dom": "16.5.2",
    "react-select": "2.1.2",
    "react-spring": "6.1.8",
    "react-styleguidist": "7.3.8",
    "react-transition-group": "2.5.0",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.9",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-node-externals": "1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "scripts": {
    "start": "styleguidist server",
    "lint": "eslint ./src --ext .js --ext .jsx",
    "build": "node scripts/build.js",
    "prepare": "npm run build",
    "test": "jest --coverage",
    "styleguide": "styleguidist build"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "plugins": [
      "flowtype"
    ],
    "globals": {
      "BABEL_ENV": "production",
      "SyntheticEvent": true,
      "SyntheticAnimationEvent": true,
      "SyntheticClipboardEvent": true,
      "SyntheticCompositionEvent": true,
      "SyntheticInputEvent": true,
      "SyntheticUIEvent": true,
      "SyntheticFocusEvent": true,
      "SyntheticKeyboardEvent": true,
      "SyntheticMouseEvent": true,
      "SyntheticDragEvent": true,
      "SyntheticWheelEvent": true,
      "SyntheticTouchEvent": true,
      "SyntheticTransitionEvent": true
    },
    "env": {
      "browser": true,
      "jest": true
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "config/webpack.config.js"
        }
      }
    },
    "rules": {
      "no-use-before-define": [
        "error",
        {
          "variables": false
        }
      ],
      "import/prefer-default-export": "off",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "peerDependencies": true,
          "devDependencies": true
        }
      ],
      "no-return-assign": [
        "error",
        "except-parens"
      ],
      "no-trailing-spaces": [
        "error",
        {
          "skipBlankLines": true,
          "ignoreComments": true
        }
      ],
      "no-bitwise": [
        "error",
        {
          "allow": [
            "~"
          ]
        }
      ]
    }
  },
  "eslintIgnore": [
    "flow-typed/**"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.js"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/config/jest/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "minify",
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-flow"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      ["module-resolver", {
        "root": ["./src"],
        "alias": {
          "components": "./src/components"
        }
      }]
    ]
  }
}
