{
  "name": "mano-toolkit",
  "version": "0.1.0",
  "description": "mano-toolkit component library",
  "private": true,
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "style": "dist/index.css",
  "files": [
    "dist/*"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.6",
    "react-router-dom": "4.3.1",
    "react-select": "2.1.2",
    "react-spring": "6.1.8",
    "react-table": "6.8.6",
    "resize-observer-polyfill": "1.5.1"
  },
  "peerDependencies": {
    "react": "16.4.2",
    "react-dom": "16.5.2",
    "react-select": "2.1.2",
    "react-spring": "6.1.8",
    "react-table": "6.8.6"
  },
  "devDependencies": {
    "@manomano/eslint-config": "0.0.11",
    "@manomano/macgyver": "0.0.11",
    "@manomano/prettier-config": "0.0.11",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "husky": "1.1.4",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-styleguidist": "7.3.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint && npm run test:coverage"
    }
  },
  "scripts": {
    "start": "styleguidist server",
    "lint": "eslint ./src --ext .js --ext .jsx",
    "build:lib": "macgyver rollup -c",
    "prepare": "yarn build:lib",
    "test": "macgyver test",
    "test:coverage": "macgyver test --coverage",
    "styleguide": "styleguidist build",
    "build:demo": "styleguidist build",
    "build:prod": "styleguidist build",
    "build:preprod": "styleguidist build"
  },
  "eslintConfig": {
    "extends": "@manomano/eslint-config"
  },
  "eslintIgnore": [
    "flow-typed/**"
  ],
  "manoConfig": {
    "alias": {
      "components": "src",
      "constants": "src/constants",
      "helpers": "src/helpers",
      "styles": "src/styles"
    }
  }
}
